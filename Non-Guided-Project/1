        "categories = []       # AI category for each app\n",
        "usefulness = []       # Usefulness rating (1-5)\n",
        "has_concerns = []     # Whether you have concerns (True/False)\n",
        "\n",
        "print(\"Empty lists created!\")\n",
        "print(f\"Number of apps so far: {len(app_names)}\")"
      ]
